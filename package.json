{
  "name": "documentry",
  "description": "AI-powered OpenAPI documentation generator for Next.js applications.",
  "version": "0.0.12",
  "type": "module",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "bin": {
    "documentry": "dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./cli": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "openapi",
    "api",
    "documentation",
    "docs",
    "generator",
    "swagger",
    "typescript",
    "nextjs",
    "nodejs",
    "cli",
    "ai",
    "anthropic",
    "openai"
  ],
  "author": "Thiago Barbosa",
  "repository": "https://github.com/thiagobarbosa/documentry",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "commander": "^11.1.0",
    "dotenv": "^16.5.0",
    "glob": "^10.4.5",
    "js-yaml": "^4.1.0",
    "openai": "^4.93.0",
    "openapi3-ts": "^4.4.0",
    "zod": "^3.24.3",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.17.30",
    "jest": "^30.0.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}

{
  "name": "nextjs-openapi-generator",
  "version": "1.0.0",
  "description": "Automatically generate OpenAPI specs from Next.js API routes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "nextjs-openapi-generator": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest"
  },
  "keywords": [
    "nextjs",
    "openapi",
    "swagger",
    "api",
    "documentation",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "glob": "^10.4.5",
    "js-yaml": "^4.1.0",
    "openapi3-ts": "^4.4.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.5",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.17.30",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}

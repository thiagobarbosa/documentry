{
  "name": "nextjs-genspec-test",
  "description": "Automatically generate OpenAPI specs from Next.js API routes using LLM models.",
  "version": "0.0.3",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "nextjs-openapi-generator": "dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts"
  },
  "keywords": [
    "nextjs",
    "openapi",
    "swagger",
    "api",
    "documentation",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.19.0",
    "commander": "^11.1.0",
    "glob": "^10.4.5",
    "js-yaml": "^4.1.0",
    "openai": "^4.93.0",
    "openapi3-ts": "^4.4.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.17.30",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "tsx": "^4.7.1"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
